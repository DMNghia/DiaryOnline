<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>checkLogin</servlet-name>
        <servlet-class>controller.checkLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Diary</servlet-name>
        <servlet-class>controller.DiaryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Signin</servlet-name>
        <servlet-class>controller.Signin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WriteDiary</servlet-name>
        <servlet-class>controller.WriteDiary</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createProfile</servlet-name>
        <servlet-class>controller.createProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>controller.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifyDiary</servlet-name>
        <servlet-class>controller.ModifyDiary</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DiaryShared</servlet-name>
        <servlet-class>controller.DiaryShared</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DiaryPostShared</servlet-name>
        <servlet-class>controller.DiaryPostShared</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewDiaryPostShared</servlet-name>
        <servlet-class>controller.viewDiaryPostShared</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DiaryFavorite</servlet-name>
        <servlet-class>controller.DiaryFavorite</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>checkLogin</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Diary</servlet-name>
        <url-pattern>/diary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signin</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WriteDiary</servlet-name>
        <url-pattern>/writeDiary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createProfile</servlet-name>
        <url-pattern>/createProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifyDiary</servlet-name>
        <url-pattern>/modifyDiary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiaryShared</servlet-name>
        <url-pattern>/diaryShared</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiaryPostShared</servlet-name>
        <url-pattern>/diaryPostShared</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewDiaryPostShared</servlet-name>
        <url-pattern>/viewDiaryPostShared</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiaryFavorite</servlet-name>
        <url-pattern>/diaryFavorite</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
